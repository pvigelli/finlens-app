{
  "cli": {
    "version": ">= 3.13.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "releaseChannel": "preview",
      "distribution": "internal",
      "developmentClient": false,
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "releaseChannel": "production",
      "distribution": "store",
      "developmentClient": false,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_USE_DEV_SERVER": "false"
      }
    }
  },
  "submit": {
    "production": {}
  }
}